generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("LOCAL_DATABASE_URL")
}

model Users {
  id              String   @id @default(uuid())
  clerk_id        String   @unique
  first_name      String
  last_name       String
  email           String   @unique
  profile_picture String?
  created_at      DateTime @default(now())
}
